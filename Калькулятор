import tkinter
from tkinter import *










tost = ""
tost2 = ""
test2 = ""
test = ""
pmud = ""
click = 0
num1 = 0
num2 = 0
nun = ""

# Это введение в строку + - * /

def clickpl():
    global pmud
    pmud = "+"
    ent.insert(END, "+")
    print("Вы нажали на +")
    global num1
    global test
    test = ent.get()
    if test:
        global tost
        tost = ent.get()
        test = (tost)
        num1 = int((test[:-1]))
    num1 = int((test[:-1]))
    print(num1)



def clickmi():
    global pmud
    pmud = "-"
    ent.insert(END, "-")
    print("Вы нажали на -")
    global num1
    global test
    test = ent.get()
    if test:
        global tost
        tost = ent.get()
        test = (tost)
        num1 = int((test[:-1]))
        print(num1)
    num1 = int((test[:-1]))
    print(num1)


def clickum():
    global pmud
    pmud = "*"
    ent.insert(END, "*")
    print("Вы нажали на *")
    global num1
    global test
    test = ent.get()
    if test:
        global tost
        tost = ent.get()
        test = (tost)
        num1 = int((test[:-1]))
    num1 = int((test[:-1]))
    print(num1)


def clickde():
    global pmud
    pmud = "/"
    ent.insert(END, "/")
    print("Вы нажали на /")
    global num1
    global test
    test = ent.get()
    if test:
        global tost
        tost = ent.get()
        test = (tost)
        num1 = int((test[:-1]))
    num1 = int((test[:-1]))
    print(num1)


# Цифры и их присаивания


def zeroo():
    global test2
    ent.insert(END, "0")
    if num1:
        test2 = ent.get()
        global tost2

        before, sep, after = test2.partition("+")
        if sep:
            tost2 = after

        before, sep, after = test2.partition("-")
        if sep:
            tost2 = after

        before, sep, after = test2.partition("*")
        if sep:
            tost2 = after

        before, sep, after = test2.partition("/")
        if sep:
            tost2 = after

        global num2
        num2 = int(tost2)
        print(num2)


