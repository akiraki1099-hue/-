# Вычисления

def equals():
    global num1
    global num2



    if pmud == "+":
        otv = num1 + num2
        ent.delete(0, END)
        ent.insert(0, otv)
        print(otv)

    if pmud == "-":
        otv = num1 - num2
        ent.delete(0, END)
        ent.insert(0, otv)
        print(otv)

    if pmud == "*":
        otv = num1 * num2
        ent.delete(0, END)
        ent.insert(0, otv)
        print(otv)

    if pmud == "/":
        otv = num1 / num2
        ent.delete(0, END)
        ent.insert(0, otv)
        print(otv)

# Доплнительные

def C():
    ent.delete(0, END)
    global tost, tost2, test, test2, pmud, num1, num2
    num1 = 0
    num2 = 0
    tost = ""
    tost2 = ""
    test2 = ""
    test = ""
    pmud = ""









root = Tk()
root.geometry("500x500")
root.resizable(width=False, height=False)
root.config(bg="Black")
root.title("+ И -")

ent = Entry(root,
            font=("Comic Sans MS", 30),
            )
ent.place(x=23, y=30, height=50, width=450)



lbl1 = Label(root,
             text="0",
             font=("Comic Sans MS", 30),
             fg="White",
             bg="Black"
             )
lbl1.place(x=230, y=90)

# Плюс, минуС, умножение и деление

btnp = Button(root,
              text="+",
              command=clickpl,
              font=("Comic Sans MS", 20),
              bg="Green"
              )
btnp.place(x=450, y=120)

btnm = Button(root,
              text="-",
              command=clickmi,
              font=("Comic Sans MS", 20),
              bg="Red"
              )
btnm.place(x=450, y=190) # y + 70

btnu = Button(root,
              text="*",
              command=clickum,
              font=("Comic Sans MS", 20),
              bg="Yellow"
              )
btnu.place(x=450, y=260) # x + 70

btnd = Button(root,
              text="/",
              command=clickde,
              font=("Comic Sans MS", 20),
              bg="Orange"
              )
btnd.place(x=450, y=330) # x + 70

btnr = Button(root,
              text="=",
              command=equals,
              font=("Comic Sans MS", 25),
              bg="White"
              )
btnr.place(x=450, y=400)

# Цифры!

